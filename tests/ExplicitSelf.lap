{$assertions on}

{$explicitself on}

procedure TIntegerArray.Test1();
var
  A: TIntegerArray;
begin
  SetLength(A, 1);
  Assert(Length(A) = 1);

  Self.SetLength(1);
end;

{$explicitself off}

procedure TIntegerArray.Test2();
var
  A: TIntegerArray;
begin
  System.SetLength(A, 2);
  Assert(System.Length(A) = 2);

  SetLength(2);
end;

var
  Arr1, Arr2: TIntegerArray;

begin
  Arr1.Test1();
  Assert(Arr1.Length() = 1);

  Arr2.Test2();
  Assert(Arr2.Length() = 2);
end.